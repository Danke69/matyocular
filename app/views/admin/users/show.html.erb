<div>
  <h3><%= @user.name %></h3>
  <%= image_tag @user.get_profile_image(100,100) %>
  <h4>プロフィール</h4>
  <h5><%= @user.introduction %></h5>
  <h4>目標</h4>
  <h5><%= @user.goal %></h5>
  <%= link_to '削除', admin_user_path(@user), method: :delete, data: { confirm: '本当に削除しますか？' } %>
</div>

<% @posts.each do |post| %>
  <%= post.user.name %>
  <%= post.body %>
  <%= link_to '削除', admin_post_path(post), method: :delete, data: { confirm: '本当に削除しますか？' } %>
<% end %>
