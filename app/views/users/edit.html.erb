<%= form_with model: @user do |f| %>
  <label for="Name">ユーザーネーム</label>
  <%= f.text_field :name, id:"Name" %>

  <label for="profile">profile</label>
  <%= f.file_field :profile_image, id:"profile" %>

  <label for="intro">プロフィール</label>
  <%= f.text_area :introduction, id:"intro" %>

  <label for="goal">目標</label>
  <%= f.text_area :goal, id:"goal" %>

  <%= f.submit 'いっきまーす!' %>
<% end %>